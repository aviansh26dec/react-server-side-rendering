{
  "name": "react_server_client_side_rendering",
  "version": "1.0.0",
  "description": "React SSR & CSR Rendering",
  "main": "index.js",
  "scripts": {
    "webpack:client": "webpack --config webpack.client.js --mode production",
    "webpack:server": "webpack --config webpack.server.js --mode production --watch",
    "webpack:start": "nodemon --watch build --exec node build/bundle.js",
    "prod": "npm-run-all --parallel webpack:*",
    "dev-server": "rimraf client/client.bundle.js && cross-env NODE_ENV=development webpack --config webpack.config.js --mode production --open --hot && node client.js",
    "prod-server": "rimraf client/client.bundle.js && cross-env NODE_ENV=production webpack --mode production && node client.js"
  },
  "author": "Avinash Kumar Singh",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.21",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.13",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {}
}
